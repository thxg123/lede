	gpio-keys {
		status = "disabled";
		compatible = "gpio-keys";
		autorepeat;

		vol-up-key {
			gpios = <&gpio0 0x1e 0x1>;
			linux,code = <0x73>;
			label = "volume up";
		};

		back-key {
			gpios = <&gpio0 0x1c 0x1>;
			linux,code = <0x9e>;
			label = "back";
		};
	};
	
	gmac1_clkin: external-gmac1-clock {
		compatible = "fixed-clock";
		clock-frequency = <50000000>;
		clock-output-names = "gmac1_clkin";
		#clock-cells = <0x0>;
	};
	hdmi-con {
		compatible = "hdmi-connector";
		type = "a";

